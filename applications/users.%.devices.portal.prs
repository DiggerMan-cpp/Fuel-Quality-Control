            <binding>
              <reference>postData#hidden</reference>
              <expression>{requestMethod} != &quot;POST&quot;</expression>
            </binding>
            <binding>
              <reference>putData#hidden</reference>
              <expression>{requestMethod} != &quot;PUT&quot;</expression>
            </binding>
            <binding>
              <reference>putData#enabled</reference>
              <expression>{requestMethod} == &quot;PUT&quot; &amp;&amp; {enabled}</expression>
            </binding>
            <binding>
              <reference>userName#enabled</reference>
              <expression>{enableAuthentication} &amp;&amp; {enabled}</expression>
